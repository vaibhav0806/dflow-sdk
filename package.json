{
  "name": "dflow-sdk",
  "version": "0.1.3",
  "description": "Unified TypeScript SDK for DFlow on Solana - prediction markets and trading",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "tsx test/live-api-test.ts",
    "test:e2e": "tsx test/e2e-test.ts"
  },
  "keywords": [
    "dflow",
    "solana",
    "prediction-markets",
    "kalshi",
    "trading",
    "swap",
    "defi",
    "crypto",
    "sdk"
  ],
  "author": "Vaibhav",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhav0806/dflow-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/vaibhav0806/dflow-sdk/issues"
  },
  "homepage": "https://dflow-sdk.vercel.app",
  "dependencies": {
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "bs58": "^6.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
